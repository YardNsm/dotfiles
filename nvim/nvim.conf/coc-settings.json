{
  "coc.preferences.diagnostic.enable": true,

  "coc.preferences.diagnostic.errorSign": "E",
  "coc.preferences.diagnostic.warningSign": "W",
  "coc.preferences.diagnostic.hintSign": "!",
  "coc.preferences.diagnostic.infoSign": "I",

  "coc.preferences.timeout": 500,
  "coc.preferences.extensionUpdateCheck": "never",

  "coc.preferences.hoverTarget": "echo",

  "pyls.trace.server": "off",

  "coc.source.file.trimSameExts":[
    ".ts",
    ".js",
    ".tsx",
    ".jsx"
  ],

  "languageserver": {
    "stylable": {
      "module": "/usr/local/lib/node_modules/stylable-intelligence/dist/src/lib/server.js",
      "filetypes": ["stylable", "typescript.jsx", "javascript.jsx"]
    }
  }
}
